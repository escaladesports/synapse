{
  "name": "synapse-search",
  "version": "1.0.3",
  "keywords": [
    "synapse",
    "search",
    "static",
    "react"
  ],
  "main": "dist/index.js",
  "author": "Kennedy Rose <krose@escaladesports.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/escaladesports/synapse.git"
  },
  "license": "MIT",
  "scripts": {
	 "analyze": "esca-scripts analyze --react --component --input ./src/index.js",
	 "build": "run-p build:main build:browser",
    "build:main": "esca-scripts build --react --component --babel --input src --output dist --multiple --styledJsx",
    "build:browser": "esca-scripts build --browser --react --component --input ./src/index.js --output dist-browser --multiple --styledJsx --minify --component --name SynapseInject && cp src-docs/index.html dist-browser/index.html",
    "dev": "esca-scripts dev ---input './dev/dev.js, ./dev/index.html' --react --browser --styledJsx",
    "prepublishOnly": "npm run build",
    "reset": "esca-scripts reset",
    "test": "echo 'no test'",
    "travis": "opn https://travis-ci.org/escaladesports/synapse/branches"
  },
  "devDependencies": {
    "esca-scripts": "^0.14.7",
    "opn-cli": "^3.1.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "worker-loader": "^1.1.1"
  },
  "dependencies": {
    "better-react-spinkit": "^2.0.4",
    "isomorphic-fetch": "^2.2.1",
    "lunr": "^2.1.6",
    "npm-run-all": "^4.1.2",
    "statable": "^0.2.3",
    "styled-jsx": "^2.2.6"
  }
}
